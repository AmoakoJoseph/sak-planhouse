{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": ".",
  "functions": {
    "server/index.ts": {
      "includeFiles": [
        "server/**/*",
        "shared/**/*"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
