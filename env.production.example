# Production Environment Variables
# Copy this to .env.production and fill in your values

# Database
DATABASE_URL=your_production_database_url_here
PGDATABASE=your_production_db_name
PGHOST=your_production_db_host
PGUSER=your_production_db_user
PGPASSWORD=your_production_db_password
PGPORT=5432

# Paystack (use your live keys for production)
PAYSTACK_PUBLIC_KEY=pk_live_your_live_public_key_here
PAYSTACK_SECRET_KEY=sk_live_your_live_secret_key_here

# Server
PORT=5000
NODE_ENV=production

# Session (use a strong secret in production)
SESSION_SECRET=your_strong_session_secret_here
